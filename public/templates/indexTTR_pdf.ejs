<!doctype html>
<html lang="en" ng-app="TTRApp">
   <head>
      <meta charset="UTF-8">
      <meta http-equiv="X-UA-Compatible" content="IE=edge">
      <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1, minimum-scale=1">
      <meta name="description" content="Calculator 1">
      <link rel="icon" href="images/favicon.ico">
      <title>TTR Calculator</title>
      <base href="http://localhost:3000/"/>
      <link rel="stylesheet" href="css/bootstrap.min.css">
      <link rel="stylesheet" href="css/bootstrap-select.min.css">
      <link rel="stylesheet" href="css/customTTR_pdf.css">
      <link rel="stylesheet" href="css/font-awesome.css">
      <!-- <link rel="stylesheet" href="bower_components/angularPrint/angularPrint.css" > -->
      <link rel="stylesheet" href="css/nouislider.min.css">
      <link href='http://fonts.googleapis.com/css?family=Roboto' rel='stylesheet' type='text/css'>
   </head>
   <body print-section ng-cloak>
      <div class="otrp-calculator container-fluid " ng-controller="TTRController" >


               <div class="result-div">
                 <div class="row">
                    <div class="col-sm-11" id="chartBox">
                          <div id="container"  ng-class="{largeWidth: needSS}" ></div>
                    </div>
                </div>
                <!--for pdf-->
                <div class="row">
                  <div class="col-sm-8 pannel_left">
                      <div class="one">
                        <h3 class="leftHead skyBlue">Your take home money</h3>
                        <div class="row">
                            <div class="col-sm-6">
                              <p>Without Salary Sacrifice</p>
                              <p>With Salary Sacrifice</p>
                            </div>
                            <div class="col-sm-6">
                              <p class="price">{{ resultWithoutSS[0] | currency }}</p>
                              <p class="price">{{ resultWithSS[0] | currency }}</p>
                            </div>
                        </div>
                      </div>
                      <div class="two">
                        <h3 class="leftHead orange">Accumulation End Balance</h3>
                        <div class="row">
                          <div class="col-sm-6">
                            <p>Without Salary Sacrifice</p>
                            <p>With Salary Sacrifice</p>
                          </div>
                          <div class="col-sm-6">
                            <p class="price">{{ resultWithoutSS[1] | currency }}</p>
                            <p class="price">{{ resultWithSS[1] | currency }}</p>
                          </div>
                        </div>
                      </div>
                      <div class="three">
                          <div class="row ">
                              <div class="col-sm-4 ">
                                  <div class="whiteBg">
                                  <span class="rupeesImage"><img src="images/money.png"/></span>
                                  <span class="finalAmount">Final</br>Amount</span>
                                </div>
                              </div>
                              <div class="col-sm-4 paddingZero">
                                <p>Without Salary Sacrifice</p>
                                <p class="pdng-lft9">With Salary Sacrifice</p>
                              </div>
                              <div class="col-sm-4">
                                <p class="price">{{ resultWithoutSS[2] | currency }}</p>
                                <p class="price">{{ resultWithSS[2] | currency }}</p>
                              </div>
                          </div>
                      </div>
                  </div>
                  <div class="col-sm-4 paneel_right">
                      <div class="right_inner">
                        <h2>final result</h2>
                        <div ng-show="needSS">
                          <p class="margintop17">With Salary Sacrifice,</br>you save</p>
                          <h4 class="blu">{{ resultWithSS[2] -resultWithoutSS[2] | currency }}</h4>
                        </div>
                        <div ng-show="!needSS">
                            <p>Without Salary Sacrifice,you save</p>
                            <h4 class="org">{{ resultWithoutSS[2] -resultWithSS[2] | currency }}</h4>
                        </div>
                        <div ng-show="needSS">
                            <p class="margintop19">Salary Sacrifice Amount</p>
                            <h4 class="org">{{favourableSS | currency}}</h4>
                        </div>
                        <div ng-show="needSS">
                            <p class="margintop19">Drawdown Percentage</p>
                            <h4 class="org">{{(favourableDD * 100).toFixed(2)}} %</h4>
                        </div>
                      </div>
                  </div>
                </div>
                <!--end here for pdf-->
              </div>





         <script>
        var ttrObj =<%-JSON.stringify(data)%>
    </script>
      <script src="js/jquery-2.2.4.min.js" type="text/javascript"></script>
      <script src="js/angular.min.js" type="text/javascript"></script>
      <script src="js/bootstrap-select.min.js" type="text/javascript"></script>
      <script src="js/ui-bootstrap-tpls-1.3.3.min.js" type="text/javascript"></script>
      <script src="js/bootstrap.min.js" type="text/javascript"></script>
      <script src="js/app.js" type="text/javascript"></script>
      <script src="js/controllerTTR.js" type="text/javascript"></script>
      <script src="js/AgeCalculatorService.js" type="text/javascript"></script>
      <script src="js/TaxRateCalculatorService.js" type="text/javascript"></script>
      <script src="js/SGCRateCalculator.js" type="text/javascript"></script>
      <script src="js/WithoutSSCalculatorService.js" type="text/javascript"></script>
      <script src="js/WithSSCalculatorService.js" type="text/javascript"></script>
      <script src="js/nouislider.min.js" type="text/javascript"></script>
      <script src="js/hc/highcharts.js" type="text/javascript"></script>
      <script src="js/hc/highcharts-3d.js" type="text/javascript"></script>
      <script src="js/hc/data.js" type="text/javascript"></script>
      <script src="js/hc/exporting.js" type="text/javascript"></script>
      <script src="js/hc/sand-signika.js" type="text/javascript"></script>
      <script src="js/hcTTR/chartService_pdf.js" type="text/javascript"></script>
   </body>
</html>

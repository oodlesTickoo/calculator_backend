<!doctype html>
<html lang="en" ng-app="TTRApp">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1, minimum-scale=1">
    <meta name="description" content="Calculator 1">
    <link rel="shortcut icon" href="images/abc.ico" type="image/x-icon">
    <title>Super Fund Fee Comparison Calculator</title>
    <base href="http://localhost:3000/">
    <link rel="stylesheet" href="css/bootstrap.min.css">
    <link rel="stylesheet" href="css/bootstrap-select.min.css">
    <link rel="stylesheet" href="css/customSFC.css">
    <link rel="stylesheet" href="css/font-awesome.css">
    <link href='http://fonts.googleapis.com/css?family=Roboto' rel='stylesheet' type='text/css'>
</head>

<body ng-cloak>
    <div class="otrp-calculator container-fluid " ng-controller="SFCController">
        <!-- <div class=" row title ">
            <div class="col-xs-12 col-sm-12 col-lg-12" style="text-align:center">
                <h1>Super Fund Fee Comparison Calculator</h1>
            </div>
        </div> -->
        <div class="row content" style="margin-top:20px;">
            <div class="col-xs-12 col-sm-12 col-lg-12">
                <div class="row result-div secondChart">
                    <div class="col-xs-12 col-sm-7 col-md-7">
                        <div class="row">
                            <div class="col-xs-12" id="chartBox" style="padding-left:0px;">
                                <div id="container" ng-show="chartOneOpen" style="margin:0 auto;width: 327px; height: 365px;margin-top:15px;"></div>
                            </div>
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-5 col-md-5" style="padding-left:0px;padding-right:10px;">
                        <div class="row res-2">
                            <div class="col-xs-12">
                                <div class="row res-head"><strong>Discounted Super Balance</strong></div>
                                <div class="row divider"></div>
                                <div class="row dsb">
                                    <div class="col-xs-12 res-value2">{{fundA.name}}</div>
                                    <div class="col-xs-12 res-value">{{resultFundOne | currency}}</div>
                                </div>
                                <div class="row dsb">
                                    <div class="col-xs-12 res-value2">{{fundB.name}}</div>
                                    <div class="col-xs-12 res-value">{{resultFundTwo | currency}}</div>
                                </div>
                            </div>
                        </div>
                        <div class="row res-1">
                            <div class="col-xs-12">
                                <div class="row final-head"><strong>Final Result</strong></div>
                                <div class="row divider"></div>
                                <div class="row ">
                                    <div class="col-xs-12 col-sm-12 res-head">{{savingsName}} saves you</div>
                                    <div class="col-xs-12 col-sm-12 res-value dd">{{savings | currency}}</div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-xs-12">
                        <img src="images/client-logo.png" class="comp-logo" />
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script>
        var sfcObj =<%-JSON.stringify(data)%>
    </script>
    <script src="js/jquery-2.2.4.min.js" type="text/javascript"></script>
    <script src="js/angular.min.js" type="text/javascript"></script>
    <script src="js/bootstrap-select.min.js" type="text/javascript"></script>
    <script src="js/ui-bootstrap-tpls-1.3.3.min.js" type="text/javascript"></script>
    <script src="js/bootstrap.min.js" type="text/javascript"></script>
    <script src="js/app.js" type="text/javascript"></script>
    <script src="js/controllerSFC.js" type="text/javascript"></script>
    <script src="js/AgeCalculatorService.js" type="text/javascript"></script>
    <script src="js/TaxRateCalculatorService.js" type="text/javascript"></script>
    <script src="js/SGCRateCalculator.js" type="text/javascript"></script>
    <script src="js/hc/highcharts.js" type="text/javascript"></script>
    <script src="js/hc/data.js" type="text/javascript"></script>
    <script src="js/hc/exporting.js" type="text/javascript"></script>
    <script src="js/hc/sand-signika.js" type="text/javascript"></script>
    <script src="js/hcSFC/chartService.js" type="text/javascript"></script>

</body>

</html>
